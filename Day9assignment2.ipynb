    "def armstrong():\n",
    "    for i in range(1, 1000 + 1):\n",
    "        order = len(str(i))\n",
    "        s = 0\n",
    "        while temp > 0:\n",
    "            digit = temp % 10\n",
    "            s += digit ** order\n",
    "            temp //= 10\n",
    "        if i ==s:\n",
    "            yield i"
   ]
